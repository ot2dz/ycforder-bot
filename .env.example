# ⚠️ IMPORTANT: These are examples only!
# DO NOT commit real values to the repository.
# Set these as Environment Variables in Coolify at deployment time.

# Production environment settings
NODE_ENV=production
PORT=3000

# Telegram Configuration (Set in Coolify Environment Variables)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHANNEL_ID=your_channel_id_here

# Airtable Configuration (Set in Coolify Environment Variables)
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here
AIRTABLE_TABLE_NAME=your_table_name_here

# Cloudinary Configuration (Set in Coolify Environment Variables)
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_cloudinary_api_key_here
CLOUDINARY_API_SECRET=your_cloudinary_api_secret_here

# Authorization (Set in Coolify Environment Variables)
AUTHORIZED_USER_IDS=user_id_1,user_id_2,user_id_3

# Optional: Logging
LOG_LEVEL=info